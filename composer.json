{
    "name": "sumocoders/framework",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "sentry/sentry-symfony": "^3.1",
        "sumocoders/framework-core-bundle": "^3.0",
        "symfony/apache-pack": "^1.0.1",
        "symfony/debug-bundle": "^5.1",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.6.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/security-bundle": "^5.0",
        "symfony/web-profiler-bundle": "^5.1",
        "symfony/webpack-encore-bundle": "^1.6.2",
        "symfony/website-skeleton": "^5.0",
        "symfony/yaml": "^5.0",
        "tijsverkoyen/convert-to-junit-xml": "^1.7",
        "willdurand/js-translation-bundle": "^4.0"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^0.3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsoftwig/twigcs": "^5.0",
        "mglaman/phpstan-junit": "^0.12",
        "phpstan/phpstan-symfony": "^0.12.4",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/debug-pack": "*",
        "symfony/panther": "^1.0",
        "symfony/phpunit-bridge": "^5.2",
        "tijsverkoyen/deployer-sumo": "^1.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "post-create-project-cmd": "App\\Skeleton\\PostCreateProject::run"
    }
}
