{
    "name": "sumocoders/framework",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-sodium": "*",
        "symfony/flex": "^2"
    },
    "flex-require": {
        "beberlei/doctrineextensions": "^1.5",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "sentry/sentry-symfony": "^5.1",
        "sumocoders/framework-core-bundle": "^14.1",
        "symfony/apache-pack": "^1.0.1",
        "symfony/asset-mapper": "^7.3",
        "symfony/debug-bundle": "^7.3",
        "symfony/dotenv": "^7.3",
        "symfony/expression-language": "^7.3",
        "symfony/flex": "^2.4",
        "symfony/http-client": "^7.3",
        "symfony/mailer": "^7.3",
        "symfony/messenger": "^7.3",
        "symfony/monolog-bundle": "^3.10",
        "symfony/rate-limiter": "^7.3",
        "symfony/runtime": "^7.3",
        "symfony/security-bundle": "^7.3",
        "symfony/stimulus-bundle": "^2.22",
        "symfony/ux-turbo": "^2.22",
        "symfony/validator": "^7.3",
        "symfony/web-profiler-bundle": "^7.3",
        "symfony/yaml": "^7.3",
        "symfonycasts/sass-bundle": "^0.8",
        "twbs/bootstrap": "^5.3",
        "nelmio/security-bundle": "^3.5"
    },
    "require-dev": {
    },
    "flex-require-dev": {
        "dama/doctrine-test-bundle": "^8.3",
        "doctrine/doctrine-fixtures-bundle": "^3.7",
        "micheh/phpcs-gitlab": "^1.1",
        "phpstan/phpstan-symfony": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpunit/phpunit": "^12.2",
        "squizlabs/php_codesniffer": "^3.11",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "*",
        "symfony/stopwatch": "^7.3",
        "tijsverkoyen/deployer-sumo": "^4.1",
        "vincentlanglet/twig-cs-fixer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "post-create-project-cmd": "App\\Skeleton\\PostCreateProject::run"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
